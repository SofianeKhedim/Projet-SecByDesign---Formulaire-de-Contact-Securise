# Variables d'environnement pour le formulaire sécurisé
# Renommer ce fichier en .env pour l'utiliser

# Port du serveur
PORT=3000

# Secrets (utiliser des valeurs aléatoires en production)
COOKIE_SECRET=changez_moi_en_production_cookie_secret
SESSION_SECRET=changez_moi_en_production_session_secret
JWT_SECRET=changez_moi_en_production_jwt_secret
ENCRYPTION_KEY=changez_moi_en_production_encryption_key_32chars

# Configuration reCAPTCHA
RECAPTCHA_SITE_KEY=votre_cle_site_recaptcha
RECAPTCHA_SECRET_KEY=votre_cle_secrete_recaptcha

# Mode de l'application
NODE_ENV=development